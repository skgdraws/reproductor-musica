<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.newreproductormusica.SceneController">
   <children>
      <Label fx:id="songLabel" alignment="CENTER" contentDisplay="TOP" layoutY="14.0" prefHeight="74.0" prefWidth="600.0" text="Yes I did this last minute, how could u tell...">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutX="17.0" layoutY="109.0" prefHeight="18.0" prefWidth="566.0" progress="0.0" />
      <Button fx:id="prevButton" layoutX="1.0" layoutY="147.0" mnemonicParsing="false" onAction="#prevSong" prefHeight="40.0" prefWidth="85.0" text="PREVIOUS" />
      <Button fx:id="pauseButton" layoutX="86.0" layoutY="147.0" mnemonicParsing="false" onAction="#pauseSong" prefHeight="40.0" prefWidth="85.0" text="PAUSE" />
      <Button fx:id="resetButton" layoutX="342.0" layoutY="147.0" mnemonicParsing="false" onAction="#resetSong" prefHeight="40.0" prefWidth="85.0" text="RESET" />
      <Button fx:id="nextButton" layoutX="257.0" layoutY="147.0" mnemonicParsing="false" onAction="#nextSong" prefHeight="40.0" prefWidth="85.0" text="NEXT" />
      <Slider fx:id="volumeSlider" layoutX="426.0" layoutY="158.0" prefHeight="24.0" prefWidth="160.0" showTickMarks="true" value="50.0" />
      <Button fx:id="playButton1" layoutX="171.0" layoutY="147.0" mnemonicParsing="false" onAction="#playSong" prefHeight="40.0" prefWidth="85.0" text="PLAY" />
   </children>
</AnchorPane>
